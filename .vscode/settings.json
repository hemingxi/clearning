// Workspace settings
{
    // The following will hide the js and map files in the editor
    "files.exclude": {
        ".gitignore": true,
        "**/*.js": true,
        "**/*.map": true
    },

    "code-runner.executorMap": {
        "c": "cd $dir && gcc $fileName -o ./temp/$fileNameWithoutExt && $dir./temp/$fileNameWithoutExt",
    }
}